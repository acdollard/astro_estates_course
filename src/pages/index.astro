---
import "../styles/global.css";
console.log("whaddup");
const title = "HELLO WORLD";
const response = await fetch(`${import.meta.env.WPGRAPHQL_URL}`, 
{
  method: "POST",
  headers: {
    "content-type": "application/json"
  },
  body: JSON.stringify({
    query: `query PageQuery {
      nodeByUri(uri: "/") {
        ... on Page {
          id
          blocks
        }
      }

    }`
  })
});
const data = await response.json();
console.log({data});
